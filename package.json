{
  "name": "Busboy",
  "version": "1.0.0",
  "description": "TfL live bus times app",
  "main": "index.js",
  "scripts": {
    "cordova": "cordova",
    "build": "browserify  app/index.jsx -o www/js/index.js",
    "watch": "watchify -d app/index.jsx -o www/js/index.js",
    "serve": "cd www; browser-sync start --open --server --files=\"**/*\""
  },
  "browserify": {
    "transform": [
      "babelify",
      "sassify",
      "cssify"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/quarterto/Busboy.git"
  },
  "keywords": [
    "tfl",
    "bus"
  ],
  "author": "Matt Brennan <mwrbrennan@gmail.com> (http://blog.153.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quarterto/Busboy/issues"
  },
  "homepage": "https://github.com/quarterto/Busboy",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browser-sync": "^1.8.3",
    "browserify": "^13.1.1",
    "cordova": "^4.1.2",
    "cssify": "^0.6.0",
    "ios-deploy": "^1.3.2",
    "ios-sim": "^3.1.1",
    "reactify": "^0.17.1",
    "sassify": "^0.5.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "baconjs": "^0.7.41",
    "classnames": "^2.2.5",
    "material-design-fonticons": "^1.0.0",
    "moment": "^2.8.4",
    "mt-latlon": "0.0.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "tfl-busboy": "^1.1.1",
    "underscore": "^1.7.0"
  }
}
