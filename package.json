{
  "name": "Busboy",
  "version": "1.0.0",
  "description": "TfL live bus times app",
  "main": "index.js",
  "scripts": {
    "cordova": "cordova",
    "build": "browserify  app/index.js -o www/js/index.js",
    "watch": "watchify -d app/index.js -o www/js/index.js"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ],
      "sassify",
      "cssify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/quarterto/Busboy.git"
  },
  "keywords": [
    "tfl",
    "bus"
  ],
  "author": "Matt Brennan <mwrbrennan@gmail.com> (http://blog.153.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quarterto/Busboy/issues"
  },
  "homepage": "https://github.com/quarterto/Busboy",
  "devDependencies": {
    "browserify": "^8.0.2",
    "cordova": "^4.1.2",
    "cssify": "^0.6.0",
    "ios-deploy": "^1.3.2",
    "ios-sim": "^3.1.1",
    "reactify": "^0.17.1",
    "sassify": "^0.5.2",
    "watchify": "^2.2.1"
  },
  "dependencies": {
    "baconjs": "^0.7.41",
    "material-design-fonticons": "^1.0.0",
    "mt-latlon": "0.0.3",
    "react": "^0.12.2",
    "react-bacon": "0.0.4",
    "react-tap-event-plugin": "^0.1.3",
    "tfl-busboy": "^1.0.3",
    "underscore": "^1.7.0"
  }
}
