{
  "name": "Busboy",
  "version": "1.0.0",
  "description": "TfL live bus times app",
  "scripts": {
    "analyse": "NODE_ENV=production browserify --node -p discify app/index.jsx > /dev/null",
    "postanalyse": "open disc/map.html",
    "start": "server.js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/quarterto/Busboy.git"
  },
  "keywords": [
    "tfl",
    "bus"
  ],
  "author": "Matt Brennan <mwrbrennan@gmail.com> (http://blog.153.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quarterto/Busboy/issues"
  },
  "homepage": "https://github.com/quarterto/Busboy",
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browser-sync": "^1.8.3",
    "browserify": "^13.1.1",
    "cordova": "^4.1.2",
    "cssify": "^0.6.0",
    "disc": "^1.3.2",
    "discify": "^1.4.2",
    "ios-deploy": "^1.3.2",
    "ios-sim": "^3.1.1",
    "reactify": "^0.17.1",
    "sassify": "^0.5.2",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "baconjs": "^0.7.41",
    "browserify-middleware": "^7.1.0",
    "classnames": "^2.2.5",
    "express": "^4.14.0",
    "express-http-proxy": "^0.10.1",
    "morgan": "^1.7.0",
    "mt-latlon": "0.0.3",
    "node-sass": "^3.13.0",
    "node-sass-middleware": "^0.10.0",
    "preact": "^6.4.0",
    "serve-favicon": "^2.3.2",
    "sw-toolbox": "^3.4.0",
    "tfl-busboy": "^1.2.0"
  }
}
